.page-catalogue {
    .catalogue-container {
        padding: 0;

        .catalogue-content {
            border: 1px solid var(--color-grey-lighter);
            border-radius: 5px;
        }
    }
}

@media (min-width: 768px) {
    .page-catalogue {
        .catalogue-container {
            padding: 5px 0 5px 5px;
        }
    }
}
